{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "installation": {
    "version": "19.6.4",
    "plugins": {
      "@nx/eslint": "19.6.4"
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": false
    },
    "install": {
      "cache": false
    },
    "install:clean": {
      "cache": false
    },
    "setup": {
      "dependsOn": [
        "^setup"
      ],
      "cache": false
    }
  },
  "projects": {
    "api-gateway": {
      "root": "api-gateway",
      "sourceRoot": "api-gateway/src",
      "projectType": "application",
      "targets": {
        "setup": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace api-gateway run setup",
            "cwd": "api-gateway"
          },
          "dependsOn": [
            "^setup"
          ]
        },
        "install:clean": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace api-gateway run install:clean",
            "cwd": "api-gateway"
          }
        },
        "build": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace api-gateway run build"
          }
        },
        "dev": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace api-gateway run dev"
          }
        }
      },
      "implicitDependencies": [
        "shared-rabbitmq-module",
        "event-module"
      ]
    },
    "file-upload": {
      "root": "file-upload",
      "sourceRoot": "file-upload/src",
      "projectType": "application",
      "targets": {
        "setup": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace file-upload run setup",
            "cwd": "file-upload"
          },
          "dependsOn": [
            "^setup"
          ]
        },
        "install:clean": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace file-upload run install:clean",
            "cwd": "file-upload"
          }
        },
        "build": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace file-upload run build"
          }
        },
        "dev": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace file-upload run dev"
          }
        }
      },
      "implicitDependencies": [
        "shared-rabbitmq-module",
        "event-module"
      ]
    },
    "image-compression": {
      "root": "image-compression",
      "sourceRoot": "image-compression/src",
      "projectType": "application",
      "targets": {
        "setup": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace image-compression run setup",
            "cwd": "image-compression"
          },
          "dependsOn": [
            "^setup"
          ]
        },
        "install:clean": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace image-compression run install:clean",
            "cwd": "image-compression"
          }
        },
        "build": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace image-compression run build"
          }
        },
        "dev": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace image-compression run dev"
          }
        }
      },
      "implicitDependencies": [
        "shared-rabbitmq-module",
        "event-module"
      ]
    },
    "image-resolution-conversion": {
      "root": "image-resolution-conversion",
      "sourceRoot": "image-resolution-conversion/src",
      "projectType": "application",
      "targets": {
        "setup": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace image-resolution-conversion run setup",
            "cwd": "image-resolution-conversion"
          },
          "dependsOn": [
            "^setup"
          ]
        },
        "install:clean": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace image-resolution-conversion run install:clean",
            "cwd": "image-resolution-conversion"
          }
        },
        "build": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace image-resolution-conversion run build"
          }
        },
        "dev": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace image-resolution-conversion run dev"
          }
        }
      },
      "implicitDependencies": [
        "shared-rabbitmq-module",
        "event-module"
      ]
    },
    "web": {
      "root": "web",
      "projectType": "application",
      "targets": {
        "dev": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "web:build",
            "dev": true
          }
        },
        "build": {
          "executor": "@nrwl/next:build",
          "options": {
            "outputPath": "dist/apps/web"
          }
        },
        "start": {
          "executor": "@nrwl/next:start",
          "options": {
            "buildTarget": "web:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "web/**/*.ts",
              "web/**/*.tsx"
            ]
          }
        }
      }
    },
    "event-module": {
      "root": "libs/event-module",
      "sourceRoot": "libs/event-module/src",
      "projectType": "library",
      "targets": {
        "setup": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn run setup",
            "cwd": "libs/event-module"
          }
        },
        "install:clean": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn run install:clean",
            "cwd": "libs/event-module"
          }
        },
        "build": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace event-module run event-module:build"
          }
        }
      }
    },
    "shared-rabbitmq-module": {
      "root": "libs/shared-rabbitmq-module",
      "sourceRoot": "libs/shared-rabbitmq-module/src",
      "projectType": "library",
      "targets": {
        "setup": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn run setup",
            "cwd": "libs/shared-rabbitmq-module"
          }
        },
        "install:clean": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn run install:clean",
            "cwd": "libs/shared-rabbitmq-module"
          }
        },
        "build": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn workspace shared-rabbitmq-module run shared-rabbitmq:build"
          }
        }
      }
    }
  },
  "defaultBase": "main"
}
